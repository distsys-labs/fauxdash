{
  "name": "fauxdash",
  "version": "1.8.3",
  "description": "I don't know it's not lodash",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "semistandard \"src/**/*.ts\" \"vitest.config.ts\"",
    "lint-fix": "semistandard --fix \"src/**/*.ts\" \"vitest.config.ts\"",
    "pretest": "npm run lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/deftly/fauxdash.git"
  },
  "author": "Alex Robson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deftly/fauxdash/issues"
  },
  "homepage": "https://github.com/deftly/fauxdash#readme",
  "semistandard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "env": [
      "node"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint-import-resolver-node": "^0.3.9",
    "semistandard": "^17.0.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
